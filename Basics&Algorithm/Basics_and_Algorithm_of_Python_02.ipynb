    "# k번 째 약수\n",
    "data = list(map(int, input().split()))\n",
    "save = []\n",
    "for i in range(1, data[0]+1):\n",
    "    if data[0] % i == 0:\n",
    "        save.append(i)\n",
    "print(save[-data[1]])"
